DOMO_PATH=/usr/local/sbin/domo

do_start() {

}

do_start() {

}

case "$1" in
        start)
                do_start
                ;;
        stop)
                do_stop
                ;;
        restart)
                do_stop
                do_start
                ;;
        status)
                exit 0
                ;;
        *)
                echo "Usage: /etc/init.d/domo  {start|stop|restart} "
                ;;
esac
